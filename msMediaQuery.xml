      <Part type="8e9e4a7c-a4d3-4c36-8e8e-fb6702402f63">
        <Source><![CDATA[<Definition auto="false" >

<Script Name="initialGetMediaQuery" When="AfterShow">

	if (!this.IsPostBack) {
	
		gx.http.loadScript("MatiasSolucoes/msMediaQuery/msMediaQuery_getMediaQuery.min.js", (function () {
						
			$(window).on('breakpoint-change', function(e, breakpoint) {
				
			    if(breakpoint === 'iPhone-Retina') {
										
					gx.fx.obs.notify("msMediaQueryAPI.OnChange", ["iPhone-Retina"]);
					
			    }
				
			    if(breakpoint === 'Extra-Small') {
					
					gx.fx.obs.notify("msMediaQueryAPI.OnChange", ["Extra-Small"]);
					
			    }
				
			    if(breakpoint === 'Small') {
					
					gx.fx.obs.notify("msMediaQueryAPI.OnChange", ["Small"]);
					
			    }
			    			    	
				if(breakpoint === 'Medium') {
					
					gx.fx.obs.notify("msMediaQueryAPI.OnChange", ["Medium"]);
					
			    }
			    			    
			    if(breakpoint === 'Large') {
					
					gx.fx.obs.notify("msMediaQueryAPI.OnChange", ["Large"]);
					
			    }
				
			});
			
		}).closure(this));
	}

</Script>

</Definition >]]></Source>
